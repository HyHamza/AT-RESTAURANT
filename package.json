{
  "name": "at-restaurant",
  "version": "1.0.0",
  "private": true,
  "description": "Complete restaurant ordering system with Next.js and Supabase",
  "keywords": [
    "restaurant",
    "ordering",
    "nextjs",
    "supabase",
    "pwa",
    "offline"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build && node src/generate-sw-manifest.js",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "preview": "next build && next start",
    "clean": "rm -rf .next out node_modules/.cache",
    "generate-sw-manifest": "node scripts/generate-sw-manifest.js",
    "verify-pwa": "node scripts/verify-dual-pwa.js",
    "validate-pwa": "node scripts/validate-dual-pwa.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dexie": "^4.2.1",
    "lucide-react": "^0.563.0",
    "next": "^15.5.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
